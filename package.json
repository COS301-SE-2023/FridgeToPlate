    "@nrwl/web": "15.9.3",
    "@nrwl/workspace": "15.9.2",
    "@nxrocks/nx-spring-boot": "^7.0.0",
    "@schematics/angular": "~15.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.0",
